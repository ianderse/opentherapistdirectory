<div class="therapist-container">
   <div class="large-10 push-1 columns" style="margin-top:40px">
    <%= image_tag @therapist.picture.url(:medium), class: 'therapist-img-med' %>
    <h2><%= @therapist.full_name %> <%= @therapist.certifications %>
      <div class="th-show-phone"><%= @therapist.phone %></div></h2>
    <div class="th-show-email"><%= mail_to @therapist.email %></div>
    <div class="th-show-description"><%= @therapist.description %></div>
  </div>
</div>
