<!DOCTYPE html>
<html>
	<head>
	  <title>Open Therapist Directory</title>
	  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
	  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	  <%= csrf_meta_tags %>
	  <script src="assets/list.min.js" type="text/javascript"></script>
	 	<script src="assets/list.pagination.min.js" type="text/javascript"></script>
	 	<script src="assets/jquery.stellar.min.js" type="text/javascript"></script>
	 	<script src="assets/foundation.min.js" type="text/javascript"></script>

	 	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	</head>
	<body>
	  <div id="main-header">
			<%= render "partials/header" %>
		</div>

		<%= yield %>
		<%= render 'partials/footer' %>
	</body>
</html>
